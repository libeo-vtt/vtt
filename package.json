{
  "name": "vtt-starter-kit",
  "version": "0.2.1",
  "description": "Starter kit for VTT projects",
  "url": "http://127.0.0.1:3000",
  "keywords": "vtt, starter, kit",
  "repository": "https://gitlab.libeo.com/vtt/starter-kit",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./src/js/vendor/jquery.min.js",
    "onfontresize": "./src/js/vendor/jquery.onfontresize.js",
    "upload": "./src/js/vendor/jquery.upload.js",
    "webfont": "./src/js/vendor/webfont.min.js",
    "waitforimages": "./src/js/vendor/jquery.waitforimages.min.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "onfontresize": {
      "depends": "jquery",
      "exports": null
    },
    "upload": {
      "depends": "jquery",
      "exports": null
    },
    "webfont": {
      "exports": "WebFont"
    },
    "waitforimages": {
      "depends": "jquery",
      "exports": null
    }
  },
  "devDependencies": {
    "browser-sync": "~1.9.0",
    "browserify": "^7.0.3",
    "browserify-shim": "^3.8.2",
    "cheerio": "^0.18.0",
    "del": "^1.1.1",
    "event-emitter": "^0.3.2",
    "gulp": "~3.8.10",
    "gulp-cheerio": "^0.5.1",
    "gulp-compass": "^2.0.3",
    "gulp-if": "^1.2.5",
    "gulp-prettify": "^0.3.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-svg-sprite": "^1.0.11",
    "gulp-svg2png": "^0.3.0",
    "gulp-svgmin": "^1.1.0",
    "gulp-symlink": "^2.1.0",
    "gulp-twig": "^0.1.0",
    "gulp-uglify": "^1.1.0",
    "gulp-watch": "^3.0.0",
    "inherits": "^2.0.1",
    "lodash": "^3.0.0",
    "lorem-ipsum": "^1.0.1",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "source-map": "^0.1.43",
    "svgo": "^0.5.0",
    "vinyl": "~0.4.6",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "vinyl-transform": "^1.0.0"
  }
}
